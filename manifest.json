{
  "manifest_version": 3,
  "name": "Current Opener",
  "version": "0.0.1",
  "permissions": ["nativeMessaging"],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": [
        "https://www.notion.so/*",
        "https://note.youdao.com/*"
      ],
      "js": ["content.js"],
      "all_frames": true,
      "run_at": "document_start"
    }
  ]
}

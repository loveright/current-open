{
  "manifest_version": 3,
  "name": "Current Open",
  "version": "0.1.0",
  "permissions": ["nativeMessaging", "tabs"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": [
        "https://www.notion.so/*",
        "https://note.youdao.com/*"
      ],
      "js": ["content.js"],
      "all_frames": true,
      "run_at": "document_start"
    }
  ]
}
